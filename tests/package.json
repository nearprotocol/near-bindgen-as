{
  "name": "near-bindgen",
  "version": "0.0.0",
  "description": "Tests for near bindings",
  "main": "test.js",
  "scripts": {
    "build": "../node_modules/.bin/asc assembly/test.ts --explicitStart --transform ../src/index.ts --validate --runtime none --binaryFile out/test.wasm --textFile out/test.wat --debug",
    "test": "yarn build && yarn test:wasm",
    "test:ci": "yarn test:asconfig && yarn test",
    "test:wasm": "node --experimental-wasm-bigint ./test.js",
    "build:asconfig": "node asconfig.js",
    "test:asconfig": "yarn build:asconfig && yarn test:wasm"
  },
  "dependencies": {
    "near-bindgen-as": "../"
  }
}
